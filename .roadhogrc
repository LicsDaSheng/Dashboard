{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "autoprefixer": null,
  "extraBabelPlugins": [
    "transform-runtime",
    "dva-hmr",
   ["import", { "libraryName": "antd", "style": "css" }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080/",
      "changeOrigin": true
    }
  }
}
